#!/bin/bash

#reads the file. can write to the file
#backuprw: a duplicate of arpwatch.rw

file=backuprw
while IFS= read  -r f1 f2 f3 f4
do 
	printf ' %s %s %s %s\n' "$f1" "$f2" "$f3" "$f4"
done < "$file"  

#sort the file according to the third field: time in seconds 
sort -k3 -n backuprw

